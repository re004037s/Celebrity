<% if @course == 'bootstrap_ga_1' %>
  <% if @status == 'false' %>
    $("#bootstrap_ga_1").html("<%= j(render('bootstrap_statuses/incomplete', course: @course)) %>");
  <% else %>
    $("#ruby_ga_1").html("<%= j(render('bootstrap_statuses/complete', course: @course)) %>");
  <% end %>
<% end %>

