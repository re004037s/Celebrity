<% if @course == 'html_css_ga_be' %>
  <% if @status == 'false' %>
    $("#html_css_ga_be").html("<%= j(render('html_css_statuses/incomplete', course: @course)) %>");
  <% else %>
    $("#html_css_ga_be").html("<%= j(render('html_css_statuses/complete', course: @course)) %>");
  <% end %>
<% end %>

