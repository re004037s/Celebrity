<% if @course == 'javascript_ga_be' %>
  <% if @status == 'false' %>
    $("#javascript_ga_be").html("<%= j(render('javascript_statuses/incomplete', course: @course)) %>");
  <% else %>
    $("#javascript_ga_be").html("<%= j(render('javascript_statuses/complete', course: @course)) %>");
  <% end %>
<% end %>

