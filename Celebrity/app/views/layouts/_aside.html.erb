<aside class="col-sm-3">
  <div class="panel panel-default">
    <div class="sidebar-wrapper">
      <div class="panel-heading">
        Menu
      </div>
      <ul class="nav nav-pills nav-stacked sidebar-nav">
        <% @categories_all.each do |category| %>
        <% current_user_railstutorial = current_user.railstutorial_status %>
          <li><%= link_to category.name, movie_category_path(category), class: "#{'disabled' unless before_category_comp?(category)}" %></li>
        <% end %>
        <li><%= link_to 'Progate進捗入力', progate_path %></li>
        <li><%= link_to_if(current_user_railstutorial.schedule_date.present?,  'RailsTutorial進捗入力', railstutorial_path) %></li>
        <li><%= link_to 'よくある質問', qa_path %></li>
      </ul>
    </div>
  </div>
</aside>