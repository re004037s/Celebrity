<h2 class="page-title">
  <span>新規登録</span>
</h2>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@qiitapost) do |f| %>
      <%= render partial: 'shared/error_messages', locals: {object: @qiitapost } %>
      
      <%= f.label :title, "タイトル", class: 'txt1' %>
      <%= f.text_field :title, class: 'form-control' %>
      
      <%= f.label :url, '投稿URL', class: 'txt1' %>
      <%= f.text_field :url, class: 'form-control' %>
      
      <div class="center">
        <%= f.submit '登録', {class: 'btn btn-primary post-regist' }%>
      </div>
    <% end %>
  </div>
</div>